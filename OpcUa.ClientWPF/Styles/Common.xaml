<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:OpcUa.ClientWPF.Styles">

    <!--  Colors  -->
    <Color x:Key="ColorPrimary1">Black</Color>
    <Color x:Key="ColorPrimary2">#1F1F1F</Color>

    <!--  Brushes  -->
    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource ColorPrimary1}" />
    <SolidColorBrush x:Key="BrushPrimary2" Color="{StaticResource ColorPrimary2}" />

</ResourceDictionary>